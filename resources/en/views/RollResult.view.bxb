result-view {
  match {
    RollResultConcept (rollResult)
  }
  render {
    layout {
      section {
        content {
          single-line {
            text {
              style (Detail_M)
              value ("Sum: #{value(rollResult.sum)}")
            }
          }
          single-line {
            text {
              style (Detail_M)
              value ("Rolls: #{value(rollResult.roll)}")
            }
          }
        }
      }
    }
  }
}